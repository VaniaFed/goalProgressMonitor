{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "styled-jsx/babel",
    "@babel/plugin-proposal-class-properties",
    ["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": true }],
  ],
  "env": {
    "server": {
      "plugins": [
        [
          "css-modules-transform",
          {
            "generateScopedName": "[hash:8]",
            "extensions": [
              ".css", ".sass"
            ]
          }
        ]
      ]
    }
  }
}
